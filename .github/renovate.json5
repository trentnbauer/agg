{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        ":disableRateLimiting"
    ],
    "docker-compose": {
        "fileMatch": ["docker-compose/.+\\.ya?ml$"]
    },
    "packageRules": [
        {
            "matchDatasources": ["docker"],
            "matchPackageNames": ["willfarrell/autoheal"],
            "automerge": true,
            "automergeType": "branch"
        }
    ]
}
